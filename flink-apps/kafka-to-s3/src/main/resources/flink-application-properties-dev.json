[
  {
    "PropertyGroupId": "KafkaSource",
    "PropertyMap": {
      "bootstrap.servers": "your-kafka-broker-1:9096,your-kafka-broker-2:9096,your-kafka-broker-3:9096",
      "topic": "your-kafka-topic",
      "group.id": "your-consumer-group",
      "security.protocol": "SASL_SSL",
      "sasl.mechanism": "SCRAM-SHA-512",
      "sasl.jaas.config": "org.apache.kafka.common.security.scram.ScramLoginModule required username=\"your-username\" password=\"your-password\";"
    }
  },
  {
    "PropertyGroupId": "S3Sink",
    "PropertyMap": {
      "output.path": "s3://your-bucket-name/data/",
      "file.prefix": "kafka-data",
      "file.suffix": ".json",
      "rollover.interval.minutes": "2",
      "inactivity.interval.seconds": "30",
      "max.part.size.mb": "10"
    }
  }
]
